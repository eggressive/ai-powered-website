# Environment Configuration Example
# Copy this file to .env.local and fill in your values

# API Configuration
VITE_API_BASE_URL=http://localhost:5000/api
VITE_API_TIMEOUT=10000

# Application Configuration
VITE_APP_NAME=AI Intent Tracker
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=AI-powered user intent prediction and tracking

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG=false
VITE_ENABLE_CONSENT_BANNER=true

# Analytics Configuration
VITE_ANALYTICS_UPDATE_INTERVAL=5000
VITE_SESSION_TIMEOUT=3600000

# Development Settings
VITE_MOCK_API=false
VITE_LOG_LEVEL=info

# Production Settings (uncomment for production)
# VITE_API_BASE_URL=https://api.yourproduction.com/api
# VITE_ENABLE_DEBUG=false
# VITE_LOG_LEVEL=error
# VITE_MOCK_API=false
